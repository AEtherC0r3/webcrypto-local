{
  "name": "@webcrypto-local/server",
  "version": "1.0.2",
  "description": "A package of `webcrypto-local` framework. WebSocket server",
  "keywords": [
    "webcrypto",
    "server",
    "websocket"
  ],
  "author": "PeculiarVentures",
  "contributors": [
    "Miroshin Stepan<microshine@mail.ru>"
  ],
  "homepage": "https://github.com/PeculiarVentures/webcrypto-local/tree/master/packages/server#readme",
  "license": "MIT",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/webcrypto-local.git"
  },
  "scripts": {
    "test": "mocha",
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup -c",
    "build:types": "tsc --declaration --outDir types --emitDeclarationOnly",
    "clear": "rimraf build types"
  },
  "bugs": {
    "url": "https://github.com/PeculiarVentures/webcrypto-local/issues"
  },
  "dependencies": {
    "2key-ratchet": "^1.0.14",
    "@webcrypto-local/cards": "file:../cards",
    "@webcrypto-local/core": "file:../core",
    "@webcrypto-local/proto": "file:../proto",
    "asn1js": "^2.0.22",
    "graphene-pk11": "^2.1.1",
    "node-webcrypto-p11": "^2.0.2",
    "pkijs": "^2.1.75",
    "pvtsutils": "^1.0.4",
    "pvutils": "^1.0.17",
    "request": "^2.88.0",
    "webcrypto-core": "^1.0.6",
    "ws": "^6.1.4"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "rollup": "^1.3.0",
    "rollup-plugin-typescript": "^1.0.0",
    "typescript": "^3.3.3333"
  }
}
